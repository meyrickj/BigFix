extract bomgar-scc-w0dc301g6xxdhwfwj7dyx6dizjhwgg666hhi8jc40jc90dmg.tmp

// mount the disk image 
wait hdiutil attach -noverify -nobrowse /Users/Shared/bomgar-scc-w0dc301g6xxdhwfwj7dyx6dizjhwgg666hhi8jc40jc90.dmg  

//install bomgar
wait "/Volumes/bomgar-scc/Double-Click To Start Support Session.app/Contents/MacOS/sdcust"

// unmount the volume
wait hdiutil detach -force "/Volumes/bomgar-scc"
